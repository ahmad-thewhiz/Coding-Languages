#include <iostream>

using namespace std;

int main() {

    char grade;
    cout<<"Enter Grade from A to F: ";
    cin>>grade; 

    // Switch statement to check the value of 'grade'
    switch (grade) {
        case 'A':
            cout << "Excellent!" << endl;   // Code block executed if 'grade' is 'A'
            break;
        case 'B':
            cout << "Good job!" << endl;    // Code block executed if 'grade' is 'B'
            break;
        case 'C':
            cout << "Well done!" << endl;   // Code block executed if 'grade' is 'C'
            break;
        case 'D':
            cout << "You passed." << endl;  // Code block executed if 'grade' is 'D'
            break;
        case 'F':
            cout << "Better luck next time." << endl;  // Code block executed if 'grade' is 'F'
            break;
        default:
            cout << "Invalid grade." << endl;  // Code block executed if 'grade' doesn't match any case
            break;
    }

    return 0;
}
